
@{
    ViewBag.title = "User Info";
}
<h1 style="color:greenyellow">Your Account Has Been Created Succesfully ! </h1>

@{

    var listOfUsers = Session["users"] != null ? (List<WebBankSystemApplication.Models.User>)Session["users"] : new List<WebBankSystemApplication.Models.User>();
    foreach (WebBankSystemApplication.Models.User userItem in listOfUsers)
    {
        @Html.Partial("_User", userItem)

    }
}







